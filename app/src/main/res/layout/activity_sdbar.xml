<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".sdbar">
    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="302dp"
        android:layout_height="0dp"
        android:foreground="@drawable/whitsidebar"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <RelativeLayout
        android:id="@+id/relativeLayout2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Button
            android:id="@+id/button9"
            android:layout_width="160dp"
            android:layout_height="56dp"
            android:textStyle="bold"
            android:layout_marginLeft="180dp"
            android:layout_marginTop="730dp"
            android:background="@drawable/roundedbutton"
            android:textColor="#ffffff"
            android:text="تسجيل الخروج" />

        <ImageButton
            android:id="@+id/imageButton3"
            android:layout_width="300dp"
            android:layout_height="255dp"
            android:layout_marginLeft="110dp"
            android:backgroundTint="#00FFFFFF"
            android:foreground="@drawable/wavve"
            tools:layout_editor_absoluteX="109dp" />

        <ImageButton
            android:id="@+id/imageButton4"
            android:layout_width="88dp"
            android:layout_height="97dp"
            android:layout_marginLeft="290dp"
            android:layout_marginTop="80dp"
            android:background="#00FFFFFF"
            android:foreground="@drawable/logo"
            android:src="@android:drawable/btn_star"


            />

        <ImageButton
            android:id="@+id/imageButton5"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginLeft="130dp"
            android:layout_marginTop="40dp"
            android:background="#00FFFFFF"
            android:foreground="@drawable/close" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="184dp"
            android:layout_marginTop="90dp"
            android:textSize="18sp"
            android:text="اسم المستخدم"
            android:textColor="@color/white" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="200dp"
            android:layout_marginTop="120dp"
            android:text="اسم المستخدم"
            android:textColor="@color/white" />

    </RelativeLayout>

    <GridLayout
        android:id="@+id/gridLayout8"
        android:layout_width="0dp"
        android:layout_height="611dp"
        android:layout_marginTop="256dp"
        android:columnCount="1"
        android:orientation="vertical"
        android:rowCount="6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <RelativeLayout
            android:id="@+id/laycont"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="110dp">


            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="200dp"
                android:layout_marginTop="20dp"
                android:text="تواصل معنا"
                android:textStyle="bold" />

            <ImageButton
                android:id="@+id/imageButton10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="20dp"
                android:background="#00FFFFFF"
                android:src="@drawable/vone"


                />

            <View
                android:layout_width="250dp"
                android:layout_height="2dp"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="60dp"
                android:background="#D9D9D9" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/layknmore"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="110dp"
            android:layout_marginTop="10dp">


            <TextView
                android:id="@+id/textView11"
                android:layout_width="134dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="125dp"
                android:layout_marginTop="20dp"
                android:text="الشروط والاحكام"
                android:textStyle="bold" />

            <ImageButton
                android:id="@+id/imageButton12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="20dp"
                android:background="#00FFFFFF"
                android:src="@drawable/term"


                />

            <View
                android:layout_width="250dp"
                android:layout_height="2dp"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="60dp"
                android:background="#D9D9D9" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="110dp"
            android:layout_marginTop="10dp">


            <TextView
                android:id="@+id/textView13"
                android:layout_width="134dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="125dp"
                android:layout_marginTop="20dp"
                android:text="اعرف اكثر عن التطبيق"
                android:textStyle="bold" />

            <ImageButton
                android:id="@+id/imageButton14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="20dp"
                android:background="#00FFFFFF"
                android:src="@drawable/knowmore"


                />

            <View
                android:layout_width="250dp"
                android:layout_height="2dp"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="60dp"
                android:background="#D9D9D9" />

        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/shkawy"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="110dp"
            android:layout_marginTop="10dp">


            <TextView
                android:id="@+id/textView15"
                android:layout_width="134dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="125dp"
                android:layout_marginTop="20dp"
                android:text="الشكاوى والمقترحات"
                android:textStyle="bold" />

            <ImageButton
                android:id="@+id/imageButton16"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="20dp"
                android:background="#00FFFFFF"
                android:src="@drawable/head"


                />

            <View
                android:layout_width="250dp"
                android:layout_height="2dp"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="60dp"
                android:background="#D9D9D9" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="110dp"
            android:layout_marginTop="10dp">


            <TextView
                android:id="@+id/textView17"
                android:layout_width="134dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="125dp"
                android:layout_marginTop="20dp"
                android:text="مشاركة التطبيق"
                android:textStyle="bold" />

            <ImageButton
                android:id="@+id/imageButton18"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="20dp"
                android:background="#00FFFFFF"
                android:src="@drawable/share"


                />

            <View
                android:layout_width="250dp"
                android:layout_height="2dp"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="60dp"
                android:background="#D9D9D9" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="110dp"
            android:layout_marginTop="10dp">


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp">


                <TextView
                    android:id="@+id/textView19"
                    android:layout_width="134dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="125dp"
                    android:layout_marginTop="20dp"
                    android:text="تقييم التطبيق"
                    android:textStyle="bold" />

                <ImageButton
                    android:id="@+id/imageButton20"
                    android:layout_width="22dp"
                    android:layout_height="22dp"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="20dp"
                    android:background="#00FFFFFF"
                    android:foreground="@drawable/star"


                    />

                <View
                    android:layout_width="250dp"
                    android:layout_height="2dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="60dp"
                    android:background="#D9D9D9" />

            </RelativeLayout>

        </RelativeLayout>


    </GridLayout>

</androidx.constraintlayout.widget.ConstraintLayout>